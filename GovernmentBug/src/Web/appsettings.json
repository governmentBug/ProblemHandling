{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "BugComprision": {
    "FuzzyMatchThreshold": 85
  },
  "FileExtensions": {
    "Image": [ ".jpg", ".jpeg", ".png", ".gif", ".bmp", ".webp" ],
    "Video": [ ".mp4", ".avi", ".mov", ".wmv", ".mkv", ".webm" ]
  }
}
